{% liquid 
  assign sst = section.settings
  assign bg_image = sst.background_image
%}
{% style %}
{% if request.page_type != 'index' and sst.border_top %}
  .nov-footer {
    border-top: 1px solid var(--color-border);
  }
{% endif %}
  .nov-footer {
    {% if sst.background_color != blank %}
      background: {{ sst.background_color }};
    {% endif %}
    {% if bg_image.src != blank %}
      background-image: url("{{ bg_image | image_url: width: 1920 }}");
      background-size: {{ sst.background_image_size }};
      background-position: center;
    {% endif %}
  }
  .footer_copyright {
    background: {{ sst.bg_copyright }} !important;
  }
  {% if sst.show_payment and sst.payment_mw != blank %}
    .footer_copyright .payment-content {
      max-width: {{ sst.payment_mw }}px;
    }
  {% endif %}
  {% if sst.show_copyright_border_top %}
    .copyright-content {
      border-top: 1px solid rgba(var(--color-border-rgb), {{ sst.copyright_border_top_opa }}%);
    }
  {% endif %}
  {% for block in section.blocks %}
    {% if block.type == 'block_html' %}
      .footer-layout .block-{{ forloop.index }}.footer_html img {
        margin-bottom: {{ block.settings.img_spacing }}px;
      }
    {% endif %}
  {% endfor %}
{% endstyle %}
<footer class="color-{{ sst.color_section }} color-social-{{ sst.btn_type }}">
  {% if section.blocks.size > 0 %}
    <div class="{% if sst.bg_size == 'full' %} footer-layout pt-80 pb-70 pb-xs-50 position-relative{% endif %}">
      <div class="{{ sst.container }}">
        <div class="container-inner{% if sst.bg_size == 'container' %} footer-layout pt-80 pb-70 pb-xs-50{% endif %}">
          <div class="row">
            {% for block in section.blocks %}
              {% assign bst = block.settings %}
              {% if block.type == 'block_contact' %}
                <div class="footer_contact block_footer block-{{ forloop.index }}{% if bst.block_title != blank and sst.mobile_acc %} mt-xs-0 mb-xs-0{% endif %} {{ bst.grid_xxl }} {{ bst.grid_xl }} {{ bst.grid_lg }} {{ bst.grid_md }} {{ bst.grid_sm }}{% if bst.block_class != blank %} {{ bst.block_class }}{% endif %}">
                  <div class="block h-100">
                    {% if bst.block_title != blank or bst.block_subtitle != blank %}
                      <div class="title-block{% if bst.h_t %} d-sm-none{% endif %}">
                        {{ bst.block_title }}
                        {% if sst.mobile_acc %}
                          <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                        {% endif %}
                      </div>
                      {% endif %}
                    <div class="block-content{% if bst.block_title != blank and sst.mobile_acc %} h_t{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                      <div class="pb-xs-30">
                        {% if bst.block_subtitle != blank %}
                          <div class="footer-subtitle{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">{{ bst.block_subtitle }}</div>
                        {% endif %}
                        {% if bst.image != blank %}
                          <img src="{{ bst.image | image_url: width: 200 }}" data-src="{{ bst.image | image_url: width: 500 }}" class="img-fluid lazyload" alt="{{ bst.image.alt }}" style="max-width: {{ bst.img_width }}px; margin-bottom: {{ bst.img_spacing }}px;" width="500" height="{{ 200 | divided_by: bst.image.aspect_ratio }}">
                        {% endif %}
                        {% if bst.time != blank %}
                          <div class="footertop-item__contact d-flex align-items-center">
                            <div class="footertop-item__icon"><i class="rbb-icon-store-16"></i></div>
                            <span>{{ bst.time }}</span>
                          </div>
                        {% endif %}
                        {% if bst.phone != blank %}
                          <div class="footertop-item__contact d-flex align-items-center phone">
                            <div class="footertop-item__icon"><i class="rbb-icon-phone-4"></i></div>
                            {{ 'layout.footer.call_us_now' | t }}&nbsp;<a href="tel: {{ bst.phone }}">{{ bst.phone }}</a>
                          </div>
                        {% endif %}
                        {% if bst.email != blank %}
                          <div class="footertop-item__contact d-flex align-items-center">
                            <div class="footertop-item__icon"><i class="rbb-icon-email-11"></i></div>
                            <a href="mailto:{{ bst.email }}">{{ bst.email }}</a>
                          </div>
                        {% endif %}
                        {% if bst.show_social %}
                          <div class="mt-25">{% render 'nov-social' %}</div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if block.type == 'block_menu' %}
                <div class="footer_menu block_footer block-{{ forloop.index }}{% if bst.block_title != blank and sst.mobile_acc %} mt-xs-0 mb-xs-0{% endif %} {{ bst.grid_xxl }} {{ bst.grid_xl }} {{ bst.grid_lg }} {{ bst.grid_md }} {{ bst.grid_sm }}{% if bst.block_class != blank %} {{ bst.block_class }}{% endif %}">
                  <div class="block h-100">
                    {% if bst.block_title != blank %}
                      <div class="title-block{% if bst.h_t %} d-sm-none{% endif %}">
                        {{ bst.block_title }}
                        {% if sst.mobile_acc %}
                          <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                        {% endif %}
                      </div>
                    {% endif %}
                    <div class="block-content{% if bst.block_title != blank and sst.mobile_acc %} h_t{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                      {%- assign footer_linklist = bst.menu_handle -%}
                      {% if linklists[footer_linklist].links.size > 0 %}
                        <ul class="site-footer__linklist list-unstyled pb-xs-30">
                          {% for link in linklists[footer_linklist].links %}
                            <li class="site-footer__linklist-item">
                              <a href="{{ link.url }}">{{ link.title }}</a>
                            </li>
                          {% endfor %}
                        </ul>
                      {% endif %}
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if block.type == 'block_html' %}
                <div class="footer_html block_footer block-{{ forloop.index }}{% if bst.block_title != blank and sst.mobile_acc %} mt-xs-0 mb-xs-0{% endif %} {{ bst.grid_xxl }} {{ bst.grid_xl }} {{ bst.grid_lg }} {{ bst.grid_md }} {{ bst.grid_sm }}{% if bst.block_class != blank %} {{ bst.block_class }}{% endif %}">
                  <div class="block h-100">
                    {% if bst.block_title != blank %}
                      <div class="title-block{% if bst.h_t %} d-sm-none{% endif %}">
                        {{ bst.block_title }}
                        {% if sst.mobile_acc %}
                          <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                        {% endif %}
                      </div>
                    {% endif %}
                    <div class="block-content{% if bst.block_title != blank and sst.mobile_acc %} h_t{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                      <div class="pb-xs-30">
                        {% if bst.img != blank %}
                          <img src="{{ bst.img | image_url: width: 1 }}" data-src="{{ bst.img | image_url: width: 500 }}" class="img-top lazyload mb-xs-20" alt="{{ bst.img.alt }}" width="500" height="{{ 500 | divided_by: bst.img.aspect_ratio }}" style="max-width: {{ bst.img_width }}px; height: auto;">
                        {% endif %}
                        {% if bst.html_content != blank %}
                          <div class="bl_html">{{ bst.html_content }}</div>
                        {% endif %}
                        {% if bst.show_payment and shop.enabled_payment_types %}
                          <div class="footer__payment mt-30">
                            <div class="payment-content d-flex flex-wrap">
                              {% if bst.payment != blank %}
                                <img src="{{ bst.payment | image_url: width: 500 }}" style="max-width: {{ bst.payment_mw }}px; height: auto;" alt="{{ bst.payment.alt }}" width="500" height="{{ 500 | divided_by: bst.payment.aspect_ratio }}">
                              {% else %}
                                {%- for type in shop.enabled_payment_types -%}
                                  <div class="payment__item">{{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}</div>
                                {%- endfor -%}
                              {% endif %}
                            </div>
                          </div>
                        {% endif %}
                        {% if bst.show_social %}
                          <div class="mt-25">
                            {% render 'nov-social' %}
                          </div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if block.type == 'block_newsletter' %}
                <div class="footer_newsletter block_footer block-{{ forloop.index }}{% if bst.block_title != blank and sst.mobile_acc %} mt-xs-0 mb-xs-0{% endif %} {{ bst.grid_xxl }} {{ bst.grid_xl }} {{ bst.grid_lg }} {{ bst.grid_md }} {{ bst.grid_sm }}{% if bst.block_class != blank %} {{ bst.block_class }}{% endif %}" style="--color-background: {{ bst.input_bg }}; --color-text: {{ bst.input_color }}">
                  <div class="block h-100">
                    {% if bst.block_title != blank %}
                      <div class="title-block{% if bst.h_t %} d-sm-none{% endif %}">
                        {{ bst.block_title }}
                        {% if sst.mobile_acc %}
                          <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                        {% endif %}
                      </div>
                    {% endif %}
                    <div class="block-content{% if bst.block_title != blank and sst.mobile_acc %} h_t{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                      <div class="pb-xs-30">
                        {% if bst.desc != blank %}
                          <div class="desc mb-30">{{ bst.desc }}</div>
                        {% endif %}
                        {% form 'customer', id: 'footer_contact' %}
                          {{ form.errors | default_errors }}
                          {% if form.posted_successfully? %}
                            <p class="form--success text-center">{{ 'general.newsletter_form.confirmation' | t }}</p>
                          {% else %}
                            <div class="input-group">
                              <input type="hidden" name="contact[tags]" value="newsletter">
                              <input type="email"
                                name="contact[email]"
                                id="Email_Footer"
                                class="input form-control"
                                value="{% if customer %}{{ customer.email }}{% endif %}"
                                placeholder="{{ 'general.newsletter_form.email_newsletter' | t }}"
                                required>
                                <button type="submit" name="commit" class="btn {{ bst.btn_type }}">
                                  <span>{{ 'general.newsletter_form.submit' | t }}</span>
                                </button>
                            </div>
                          {% endif %}
                        {% endform %}
                        {% if bst.show_payment and shop.enabled_payment_types %}
                          <div class="footer__payment mt-30">
                            <div class="title-block">{{ 'layout.footer.payments' | t }}</div>
                            <div class="payment-content d-flex flex-wrap">
                              {% if bst.payment != blank %}
                                <img src="{{ bst.payment | image_url: width: 420 }}" style="max-width: {{ bst.payment_mw }}px" width="420" height="300" class="img-fluid">
                              {% else %}
                                {%- for type in shop.enabled_payment_types -%}
                                  <div class="payment__item">{{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}</div>
                                {%- endfor -%}
                              {% endif %}
                            </div>
                          </div>
                        {% endif %}
                        {% if bst.show_social %}
                          <div class="mt-25">{% render 'nov-social' %}</div>
                        {% endif %}
                      </div>
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if block.type == 'block_social' %}
                <div class="footer_social block_footer block-{{ forloop.index }}{% if bst.block_title != blank and sst.mobile_acc %} mt-xs-0 mb-xs-0{% endif %} {{ bst.grid_xxl }} {{ bst.grid_xl }} {{ bst.grid_lg }} {{ bst.grid_md }} {{ bst.grid_sm }}{% if bst.block_class != blank %} {{ bst.block_class }}{% endif %}">
                  <div class="block h-100">
                    {% if bst.block_title != blank %}
                      <div class="title-block{% if bst.h_t %} d-sm-none{% endif %}">
                        {{ bst.block_title }}
                        {% if sst.mobile_acc %}
                          <span class="f_btn_sl d-sm-none"><i class="zmdi zmdi-plus"></i></span>
                        {% endif %}
                      </div>
                    {% endif %}
                    <div class="block-content{% if bst.block_title != blank and sst.mobile_acc %} h_t{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                      <div class="pb-xs-30">
                        {% if bst.payment %}
                          <div class="mb-30">
                            <div class="title-block mb-20">{{ 'layout.footer.payments' | t }}</div>
                            {% if sst.payment != blank %}
                              <img src="{{ sst.payment | image_url: width: 800 }}" alt="{{ bst.payment.alt }}" width="800" height="{{ 800 | divided_by: bst.image.aspect_ratio }}" style="height: auto;">
                            {% else %}
                              <div class="payment-content d-flex flex-wrap">
                                {%- for type in shop.enabled_payment_types -%}
                                  <div class="payment__item mt-sm-15">{{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}</div>
                                {%- endfor -%}
                              </div>
                            {% endif %}
                          </div>
                        {% endif %}
                        <div class="title-block mb-20">{{ 'layout.footer.follow_us' | t }}</div>
                        {% render 'nov-social' %}
                      </div>
                    </div>
                  </div>
                </div>
              {% endif %}

              {% if block.type == '@app' %}
                {% render block %}
              {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  {% endif %}

  {% if sst.show_footer_copyright %}
    <div class="footer_copyright text-center color-{{ sst.color_copyright }}">
      <div class="{{ sst.container }}">
        <div class="copyright-content pt-40 pb-40 d-lg-flex justify-content-between align-items-center">
          {% if sst.copyright != blank %}
            <div class="copyright text-center">{{ sst.copyright }}</div>
          {% endif %}
          {% if sst.show_payment and shop.enabled_payment_types %}
            <div class="footer__payment{% if sst.copyright != blank %} mt-md-30{% endif %}">
              <div class="payment-content d-flex flex-wrap justify-content-center">
                {% if sst.payment != blank %}
                  <img src="{{ sst.payment | image_url: width: 800 }}" alt="{{ bst.payment.alt }}" width="800" height="{{ 800 | divided_by: bst.image.aspect_ratio }}" style="height: auto;" class="img-fluid">
                {% else %}
                  <div class="payment-content d-flex flex-wrap">
                    {%- for type in shop.enabled_payment_types -%}
                      <div class="payment__item">{{ type | payment_type_svg_tag: class: 'icon icon--full-color' }}</div>
                    {%- endfor -%}
                  </div>
                {% endif %}
              </div>
            </div>
          {% endif %}
          {% if sst.show_copyright_social %}
            <div class="mt-md-40">{% render 'nov-social' %}</div>
          {% endif %}
        </div>
      </div>
    </div>
  {% endif %}
</footer>
{% schema %}
  {
    "name": "Footer",
    "class": "nov-footer",
    "settings": [
      {
        "type": "checkbox",
        "id": "animate",
        "label": "Show animated",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "border_top",
        "label": "Show border top",
        "default": false
      },
      {
        "type": "header",
        "content": "Section Style"
      },
      {
        "type": "select",
        "id": "container",
        "label": "Container",
        "default": "container",
        "options": [
          { "value": "container", "label": "Container" },
          { "value": "container-fluid", "label": "Container fluid"},
          { "value": "container-full", "label": "Container full width"}
        ]
      },
      {
        "type": "color_scheme",
        "id": "color_section",
        "label": "Color section",
        "default": "background-1"
      },
      {
        "type": "color",
        "id": "background_color",
        "label": "Background colors"
      },
      {
        "type": "image_picker",
        "id": "background_image",
        "label": "Background image"
      },
      {
        "type": "select",
        "id": "background_image_size",
        "label": "Background image size",
        "default": "cover",
        "options": [
          { "value": "cover", "label": "Cover" },
          { "value": "contain", "label": "Contain"},
          { "value": "auto", "label": "Auto"}
        ]
      },
      {
        "type": "select",
        "id": "bg_size",
        "label": "Background size",
        "default": "full",
        "options": [
          { "value": "full", "label": "Full" },
          { "value": "container", "label": "Container"}
        ]
      },
      {
        "type": "header",
        "content": "Social"
      },
      {
        "type": "select",
        "id": "btn_type",
        "label": "Social color",
        "default": "btn-primary",
        "options": [
          { "value": "btn-primary", "label": "Primary" },
          { "value": "btn-secondary", "label": "Secondary" }
        ]
      },
      {
        "type": "header",
        "content": "Copyright"
      },
      {
        "type": "checkbox",
        "id": "show_footer_copyright",
        "label": "Show copyright",
        "default": true
      },
      {
        "type": "color_scheme",
        "id": "color_copyright",
        "label": "Color",
        "default": "background-1"
      },
      {
        "type": "color",
        "id": "bg_copyright",
        "label": "Background",
        "default": "#fff"
      },
      {
        "type": "checkbox",
        "id": "show_copyright_border_top",
        "label": "Show border top",
        "default": true
      },
      {
        "type": "range",
        "id": "copyright_border_top_opa",
        "min": 10,
        "max": 100,
        "step": 5,
        "label": "Border top opacity",
        "default": 50,
        "unit": "%"
      },
      {
        "type": "text",
        "id": "copyright",
        "label": "Copyright text"
      },
      {
        "type": "checkbox",
        "id": "show_payment",
        "label": "Show payment"
      },
      {
        "type": "image_picker",
        "id": "payment",
        "label": "Payment image"
      },
      {
        "type": "number",
        "id": "payment_mw",
        "label": "Payment image max width",
        "default": 200,
        "info": "Unit px"
      },
      {
        "type": "checkbox",
        "id": "show_copyright_social",
        "label": "Show social",
        "default": true
      },
      {
        "type": "header",
        "content": "Footer Mobile"
      },
      {
        "type": "checkbox",
        "id": "mobile_acc",
        "label": "Footer mobile accordion",
        "default": true
      }
    ],
    "blocks": [
      {
        "type": "block_contact",
        "name": "Block Contact",
        "settings": [
          {
            "type": "text",
            "id": "block_title",
            "label": "Title"
          },
          {
            "type": "inline_richtext",
            "id": "block_subtitle",
            "label": "Sub title"
          },
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          },
          {
            "type": "number",
            "id": "img_width",
            "label": "Image width",
            "default": 150
          },
          {
            "type": "number",
            "id": "img_spacing",
            "label": "Image margin bottom",
            "default": 10
          },
          {
            "type": "checkbox",
            "id": "h_t",
            "label": "Hidden title large mobile",
            "info": "Breakpoint 576px ↑"
          },
          {
            "type": "inline_richtext",
            "id": "time",
            "label": "Address",
            "default": "PO Box 1622 Bamboo Street West"
          },
          {
            "type": "inline_richtext",
            "id": "phone",
            "label": "Phone number",
            "default": "0123 456 789"
          },
          {
            "type": "inline_richtext",
            "id": "email",
            "label": "Email",
            "default": "demo@gmail.com"
          },
          {
            "type": "checkbox",
            "id": "show_social",
            "label": "Show social",
            "default": true
          },
          {
            "type": "text",
            "id": "block_class",
            "label": "Class"
          },
          {
            "type": "select",
            "id": "grid_xxl",
            "label": "Width item desktop 1441px",
            "default": "col-xxl-3",
            "options": [
              {"value": "col-xxl-1", "label": "1/12"},
              {"value": "col-xxl-2", "label": "2/12"},
              {"value": "col-xxl-3", "label": "3/12"},
              {"value": "col-xxl-4", "label": "4/12"},
              {"value": "col-xxl-5", "label": "5/12"},
              {"value": "col-xxl-6", "label": "6/12"},
              {"value": "col-xxl-7", "label": "7/12"},
              {"value": "col-xxl-8", "label": "8/12"},
              {"value": "col-xxl-9", "label": "9/12"},
              {"value": "col-xxl-10", "label": "10/12"},
              {"value": "col-xxl-11", "label": "11/12"},
              {"value": "col-xxl-12", "label": "12/12"},
              {"value": "col-xxl-cus-5", "label": "1/5"}
            ]
          },
          {
            "type": "select",
