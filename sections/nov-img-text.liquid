{%- assign sst = section.settings -%}
{% style %}
  {%- assign bg_image = sst.background_image -%}
  #shopify-section-{{ section.id }} .distance {
    padding-top: {{ sst.distance_top_xs }}px;
    padding-bottom: {{ sst.distance_bottom_xs }}px;
    position: relative;
    {% if sst.background_color != blank or bg_image != blank %}
      background: {{ sst.background_color }}{% if bg_image != blank %} url("{{ bg_image | image_url: width: 1920 }}"){% endif %} no-repeat center;
      background-size: cover;
    {% endif %}
  }
  {%- if sst.gradient != blank -%}
    @media(min-width: 992px) {
      #shopify-section-{{ section.id }} .distance {
        background: {{ sst.gradient }};
      }
    }
  {%- endif -%}
  #shopify-section-{{ section.id }} .block-image {
    border-radius: {{ sst.img_radius }}px;
    background: {{ sst.bg_block_img }};
  }
  #shopify-section-{{ section.id }} .block-image .respone_image {
    border-radius: {{ sst.img_radius }}px;
    transform: translateZ(0);
    overflow: hidden;
  }
  #shopify-section-{{ section.id }} .block-text {
    padding-left: {{ sst.spacing_horizontal }}px;
    padding-right: {{ sst.spacing_horizontal }}px;
  }
  {% if sst.bg_img_text != blank or sst.bg_color_text != blank %}
    #shopify-section-{{ section.id }} .block-text-content {
      background: {{ sst.bg_color_text }}{% if sst.bg_img_text != blank  %} url("{{ sst.bg_img_text | image_url: width: 1920 }}"){% endif %};
    }
  {% endif %}
  {% if sst.enable_max_width %}
    #shopify-section-{{ section.id }} .block-image .respone_image {
      max-width: {{ sst.img_max_width }}px;
    }
  {% endif %}
  {% for block in section.blocks %}
    {% assign bst = block.settings %}
    #shopify-section-{{ section.id }} .block-{{ forloop.index }} {
      margin-bottom: {{ bst.distance_bottom_xs }}px;
    }
    {% if block.type == 'text' %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text p,
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text a {
        color: {{ bst.t_c }};
        font-size: {{ bst.t_s | divided_by: 10.0 }}rem;
        line-height: {{ bst.t_lh }};
        letter-spacing: {{ bst.t_lt }}px;
        text-transform: {{ bst.t_tt }};
        max-width: {{ bst.t_m }}px;
        display: inline-block;
      }
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text a:hover {
        color: {{ bst.c_link_h }};
      }
      {% if bst.icon != blank %}
        #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text li:before {
          content: url("{{ bst.icon | image_url: width: 15 }}");
        }
      {% endif %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text strong {
        color: {{ bst.tb_c }};
      }
    {% endif %}
    {% if block.type == 'btn' %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} .btn {
        color: {{ bst.c_btn }};
        background-color: {{ bst.b_btn }};
        border-color: {{ bst.bd_btn }};
      }
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} .btn:hover {
        color: {{ bst.c_btn_h }} !important;
        background-color: {{ bst.b_btn_h }} !important;
        border-color: {{ bst.bd_btn_h }} !important;
      }
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} .icon_play {
        color:  {{ bst.c_btn_video }};
      }
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} .icon_play:hover {
        color:  {{ bst.c_btn_video_h }};
      }
    {% endif %}
    {% if block.type == 'policy' %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.policy {
        max-width: {{ bst.p_m }}px;
      }
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.policy .title {
        color: {{ bst.c_t }} !important;
        font-size: {{ bst.t_s | divided_by: 10.0 }}rem;
      }
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.policy .desc {
        color: {{ bst.c_d }} !important;
        font-size: {{ bst.d_s | divided_by: 10.0 }}rem;
      }
      {% if bst.show_bg_icon %}
        #shopify-section-{{ section.id }} .block-{{ forloop.index }}.policy .p-icon {
          background-color: {{ bst.bg_icon }};
        }
      {% endif %}  
    {% endif %}
    {% if block.type == 'image' %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} {
        max-width: {{ bst.c_m }}px;
      }
      {% if bst.show_border %}
        #shopify-section-{{ section.id }} .block-{{ forloop.index }} .item-image {
          border: 1px solid {{ bst.c_border }};
          transition: 0.3s;
        }
        #shopify-section-{{ section.id }} .block-{{ forloop.index }} .item-image:hover {
          box-shadow: 0 8px 8px rgba(0,0,0,.05);
        }
      {% endif %}
    {% endif %}
    {% if block.type == 'contact' %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.contact {
        color: {{ bst.c_c }} !important;
        max-width: {{ bst.c_m }}px;
        font-size: {{ bst.c_s | divided_by: 10.0 }}rem;
      }
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.contact .item-icon {
        color: {{ bst.c_ic }} !important;
      }
    {% endif %}
    {% if block.type == 'newsletter' %}
    #shopify-section-{{ section.id }} .block-{{ forloop.index }}.newsletter {
      --input-color: {{ bst.input_color }};
      --input-bg: {{ bst.input_bg }};
    }
    {% endif %}
  {% endfor %}
  @media(max-width: 1199px) {
    #shopify-section-{{ section.id }} .block-text {
      padding-left: clamp(0px, {{ sst.spacing_horizontal }}px, 60px);
      padding-right: clamp(0px, {{ sst.spacing_horizontal }}px, 60px);
    }
    {% for block in section.blocks %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text p,
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text a {
        font-size: {{ block.settings.t_s_lg | divided_by: 10.0 }}rem;
      }
    {% endfor %}
  }
  @media(max-width: 991px) {
    {% for block in section.blocks %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text p,
      #shopify-section-{{ section.id }} .block-{{ forloop.index }}.nov-text a {
        font-size: {{ block.settings.t_s_md | divided_by: 10.0 }}rem;
      }
    {% endfor %}
  }
  @media(max-width: 767px) {
    #shopify-section-{{ section.id }} .block-text {
      padding-left: clamp(0px, {{ sst.spacing_horizontal }}px, 15px);
      padding-right: clamp(0px, {{ sst.spacing_horizontal }}px, 15px);
    }
  }
  @media(min-width: 768px) {
    #shopify-section-{{ section.id }} .distance {
      padding-top: {{ sst.distance_top_md }}px;
      padding-bottom: {{ sst.distance_bottom_md }}px;
    }
    {% for block in section.blocks %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} {
        margin-bottom: {{ block.settings.distance_bottom_md }}px;
      }
    {% endfor %}
  }
  @media(min-width: 992px) {
    #shopify-section-{{ section.id }} .distance {
      padding-bottom: {{ sst.distance_bottom_lg }}px;
    }
    {% for block in section.blocks %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} {
        margin-bottom: {{ block.settings.distance_bottom_lg }}px;
      }
    {% endfor %}
  }
  @media(min-width: 1200px) {
    #shopify-section-{{ section.id }} .distance {
      padding-top: {{ sst.distance_top_xl }}px;
      padding-bottom: {{ sst.distance_bottom_xl }}px;
    }
    {% for block in section.blocks %}
      #shopify-section-{{ section.id }} .block-{{ forloop.index }} {
        margin-bottom: {{ block.settings.distance_bottom_xl }}px;
      }
    {% endfor %}
  }
{% endstyle %}
<div class="distance{% if sst.class != blank %} {{ sst.class }}{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}" data-section-id="{{ section.id }}" data-section-type="nov-slick">
  <div class="{{ sst.container }}">
    {% render 'heading-section', sub_position: sst.subheading_position %}
    <div class="row justify-content-lg-center{% if sst.position == 'under' %} flex-column-reverse{% endif %}{% if sst.position_lg == 'right' %} flex-lg-row-reverse{% endif %}">
      <div class="{{ sst.width_img }}">
        <div class="block-image h-100 position-relative overflow_hidden{% if sst.image_animate %} nov-sh-image-2{% endif %}" style="--color-dots: {{ sst.c_dots }}; --color-dots-act: {{ sst.c_dots_a }}; --slider-arrow-color: {{ sst.c_nav }}; --slider-arrow-color-act: {{ sst.c_nav_a }}">
          <div class="nov-slick-carousel spacing-0" data-dots="{{ sst.show_dots }}" data-nav="{{ sst.show_nav }}" data-autoplay="{{ sst.autoplay }}"data-autoplayTimeout="6000"  data-speed="1000">
            {% if sst.image1 != blank %}
              <div class="item">
                {%- render 'nov-aspect-image', image: sst.image1 -%}
              </div>
            {% endif %}
            {% if sst.image2 != blank %}
              <div class="item">
                {%- render 'nov-aspect-image', image: sst.image2 -%}
              </div>
            {% endif %}
            {% if sst.image3 != blank %}
              <div class="item">
                {%- render 'nov-aspect-image', image: sst.image3 -%}
              </div>
            {% endif %}
            {% if sst.image1 == blank %}
              <div class="item">
                {{ 'collection-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
              </div>
            {% endif %}
          </div>
          {% if sst.img_title != blank or sst.img_subtitle != blank %}
            <div class="block-image-text position-absolute bottom-center text-center color-{{ sst.img_text_color }}">
              {% if sst.img_title != blank %}
                <div class="img-title">{{ sst.img_title }}</div>
              {% endif %}
              {% if sst.img_subtitle != blank %}
                <div class="img-subtitle">{{ sst.img_subtitle }}</div>
              {% endif %}
            </div>
          {% endif %}
        </div>
      </div>
      <div class="{{ sst.width_text }} mt-md-30">
        <div class="block-text-content position-relative pt-20 pb-20 pt-md-50 d-flex align-items-{{ sst.text_position_vertical }} justify-content-{{ sst.text_position_horizontal }} w-100 h-100{% if sst.show_boxshadow_text %} block-text-boxshadow{% endif %}" style="--shadow-corner: {{ sst.shadow_corner }}px;">
          <div class="block-text {{ sst.alignment }} position-relative">
            {% for block in section.blocks %}
              {% assign bst = block.settings %}
              {% if block.type == 'text' %}
                <div class="nov-text {{ bst.t_fw }} block-{{ forloop.index }} {{ bst.ff }}{% if bst.icon != blank %} list-icon{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">{{ bst.text }}</div>
              {% endif %}
              {% if block.type == 'btn' %}
                <div class="block-btn block-{{ forloop.index }}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                  {% if bst.btn != blank %}
                    <a href="{{ bst.btn_link }}" class="{% if bst.btn_type == 'btn' %}btn btn-{{ bst.btn_size }}{% else %}link font-700{% endif %}">
                      <span>{{ bst.btn }}</span>
                    </a>
                  {% endif %}
                  {% if bst.video_url != blank %}
                    <div class="icon_play d-inline-flex align-items-center pointer" data-toggle="modal" data-target="#ModalVideo" data-src="{% if bst.video_url.type == "youtube"  %}https://www.youtube.com/embed/{{ bst.video_url.id }}?rel=0&showinfo=0&vq=720{% endif %}{% if bst.video_url.type == "vimeo" %}//player.vimeo.com/video/{{ bst.video_url.id }}?byline=0&portrait=0&badge=0{% endif %}">
                      {{ 'sections.image_with_text.watch_video' | t }}<i class="zmdi zmdi-play-circle-outline"></i>
                    </div>
                  {% endif %}
                </div>
              {% endif %}
              {% if block.type == 'policy' %}
                <div class="policy block-{{ forloop.index }} {{ bst.style }}{% if bst.p4 != blank %} h-p4{% endif %}{% if bst.show_bg_icon %} bg_icon{% endif %} mb-sm-20">
                  {%- for i in (1..3) -%}
                    {%- assign t_id = 't' | append: i -%}
                    {%- assign d_id = 'd' | append: i -%}
                    {%- assign icon_id = 'icon' | append: i -%}
                    {% if bst[d_id] != blank or bst[icon_id] != blank %}
                      <div class="policy-item{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                        <div class="policy-item-content d-flex align-items-center">
                          {% if bst[icon_id] != blank %}
                            <div class="p-icon">
                              <img src="{{ bst[icon_id] | image_url: width: 10 }}" data-src="{{ bst[icon_id] | image_url: width: 200 }}" class="lazyload d-inline-block img-fluid" alt="{{ bst[icon_id].alt }}" width="200" height="200">
                            </div>
                          {% endif %}
                          <div class="block-right">
                            {% if bst[t_id] != blank %}
                              <div class="title">{{ bst[t_id] }}</div>
                            {% endif %}
                            {% if bst[d_id] != blank %}
                              <div class="desc mt-10">{{ bst[d_id] }}</div>
                            {% endif %}
                          </div>
                        </div>
                      </div>
                    {% endif %}
                  {%- endfor -%}
                </div>
              {% endif %}
              {% if block.type == 'contact' %}
                <div class="contact block-{{ forloop.index }}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                  {% if bst.hotline != blank %}
                    <div class="item-contact d-flex align-items-center">
                      <div class="item-icon"><i class="rbb-icon-phone-2"></i></div>
                      <span class="font-700">{{ bst.hotline }}</span>
                    </div>
                  {% endif %}
                  {% if bst.email != blank %}
                    <div class="item-contact d-flex align-items-center">
                      <div class="item-icon"><i class="rbb-icon-email-6"></i></div>
                      <span>{{ bst.email }}</span>
                    </div>
                  {% endif %}
                  {% if bst.time != blank %}
                    <div class="item-contact d-flex align-items-center">
                      <div class="item-icon"><i class="rbb-icon-time-1"></i></div>
                      <span>{{ bst.time }}</span>
                    </div>
                  {% endif %}
                </div>
              {% endif %}
              {% if block.type == 'image' %}
                <div class="images block-{{ forloop.index }}{% if sst.animate %} wow fadeInUp{% endif %} w-100" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                  <div class="nov-slick-navfor-carousel slider-shadow row" style="--slider-dot-color: {{ bst.dot_c }};--slider-dot-color-act: {{ bst.dot_c_a }}" 
                  data-autoplay="false" 
                  data-autoplayTimeout="6000" 
                  data-loop="false"
                  data-dots="{{ bst.show_dots }}"
                  data-nav="false"
                  data-row="1"
                  data-items_xl="{{ bst.xl }}"
                  data-items_lg="{{ bst.lg }}"
                  data-items_md="{{ bst.md }}"
                  data-items_sm="{{ bst.sm }}"
                  data-items_xs="{{ bst.xs }}">
                    {% for i in (1..6) %}
                      {% assign img_id = 'img'| append: i %}
                      {% assign imglink_id = 'imglink'| append: i %}
                      {% if bst[img_id] != blank %}
                        <div class="sp-item col-md-4 col-2">
                          <div class="item-image d-flex align-items-center justify-content-center">
                            {% if bst[imglink_id] != blank %}
                              <a href="{{ bst[imglink_id] }}">
                            {% endif %}
                              <img src="{{ bst[img_id] | image_url: width: 960 }}" data-src="{{ bst[img_id] | image_url: width: 960 }}" class="lazyload" alt="{{ bst[imglink_id].alt | default: 'image' }}" width="960" height="960">
                            {% if bst[imglink_id] != blank %}
                              </a>
                            {% endif %}
                          </div>
                        </div>
                      {% endif %}
                    {% endfor %}
                  </div>
                </div>
              {% endif %}
              {% if block.type == 'newsletter' %}
                <div class="newsletter block-{{ forloop.index }}{% if sst.animate %} wow fadeInUp{% endif %}" data-wow-delay="{{ forloop.index | times: 0.1 }}s">
                  {% form 'customer', id: 'img-text-newsletter' %}
                    {{ form.errors | default_errors }}
                    {% if form.posted_successfully? %}
                    <p class="form--success text-center">{{ 'general.newsletter_form.confirmation' | t }}</p>
                    {% else %}
                    <div class="input-group">
                      <input type="hidden" name="contact[tags]" value="newsletter">
                      <input type="email"
                      name="contact[email]"
                      class="form-control{% if bst.shadow %} box-shadow{% endif %}"
                      value="{% if customer %}{{ customer.email }}{% endif %}"
                      placeholder="{{ 'general.newsletter_form.email_newsletter' | t }}"
                      required
                      >
                      <button type="submit" class="newsletter__submit btn color-{{ bst.btn_color }}" name="commit" title="{{ 'general.newsletter_form.submit' | t }}">
                        <span>{{ 'general.newsletter_form.subscribe' | t }}</span>
                      </button>
                    </div>
                    {% endif %}
                  {% endform %}
                </div>
              {% endif %}
              {% if block.type == '@app' %}
                {% render block %}
              {% endif %}
            {% endfor %}
          </div>
          {% if sst.show_btn_learnmore %}
            <div class="btn-learnmore d-flex align-items-center">
              <span class="font-600 text-capitalize">{{ 'sections.button.learn_more' | t }}</span>
              <div class="btn {{ sst.btn_type }}">
                <i class="zmdi zmdi-long-arrow-right"></i>
              </div>
            </div>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</div>
{% schema %}
  {
    "name": "Vinova Image Width Text",
    "class": "section-img-text overflow_hidden",
    "settings": [
      {
        "type": "richtext",
        "id": "heading",
        "label": "Heading",
        "default": "<p>Vinova Image Width Text</p>"
      },
      {
        "type": "color",
        "id": "heading_color",
        "label": "Heading color",
        "default": "#222222"
      },
      {
        "type": "richtext",
        "id": "subheading",
        "label": "Sub heading",
        "default": "<p>Subtitle from happy customers</p>"
      },
      {
        "type": "color",
        "id": "subheading_color",
        "label": "Sub heading color",
        "default": "#666666"
      },
      {
        "type": "richtext",
        "id": "descheading",
        "label": "Description",
        "default": "<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>"
      },
      {
        "type": "color",
        "id": "descheading_color",
        "label": "Description color",
        "default": "#666666"
      },
      {
        "type": "select",
        "id": "subheading_position",
        "label": "Sub heading position",
        "default": "bottom",
        "options": [
          {
            "value": "top",
            "label": "Top"
          },
          {
            "value": "bottom",
            "label": "Bottom"
          }
        ]
      },
      {
        "type": "select",
        "id": "heading_alignment",
        "label": "Heading alignment",
        "default": "text-left",
        "options": [
          {
            "value": "text-left",
            "label": "Left"
          },
          {
            "value": "text-center",
            "label": "Center"
          },
          {
            "value": "text-right",
            "label": "Right"
          }
        ]
      },
      {
        "type":  "header",
        "content": "Block Image"
      },
      {
        "type": "image_picker",
        "id": "image1",
        "label": "Image 1"
      },
      {
        "type": "image_picker",
        "id": "image2",
        "label": "Image 2"
      },
      {
        "type": "image_picker",
        "id": "image3",
        "label": "Image 3"
      },
      {
        "type": "checkbox",
        "id": "image_animate",
        "label": "Enable image animate"
      },
      {
        "type": "inline_richtext",
        "id": "img_title",
        "label": "Title"
      },
      {
        "type": "inline_richtext",
        "id": "img_subtitle",
        "label": "Sub title"
      },
      {
        "type": "color_scheme",
        "id": "img_text_color",
        "label": "Text color",
        "default": "background-1"
      },
      {
        "type": "checkbox",
        "id": "autoplay",
        "label": "Autoplay",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "show_dots",
        "label": "Show dots",
        "default": true
      },
      {
        "type": "color",
        "id": "c_dots",
        "label": "Color dots",
        "default": "#dcdcde"
      },
      {
        "type": "color",
        "id": "c_dots_a",
        "label": "Color dots active",
        "default": "#000"
      },
      {
        "type": "checkbox",
        "id": "show_nav",
        "label": "Show nav",
        "default": true
      },
      {
        "type": "color",
        "id": "c_nav",
        "label": "Color nav",
        "default": "#dcdcde"
      },
      {
        "type": "color",
        "id": "c_nav_a",
        "label": "Color nav active",
        "default": "#000"
      },
      {
        "type": "color",
        "id": "bg_block_img",
        "label": "Background image",
        "default": "#fff"
      },
      {
        "type": "range",
        "id": "img_radius",
        "min": 0,
        "max": 40,
        "step": 1,
        "label": "Image corner",
        "default": 0,
        "unit": "px"
      },
      {
        "type": "select",
        "id": "width_img",
        "label": "Laptop",
        "info": "Breakpoint 992px ↑",
        "default": "col-lg-6",
        "options": [
          {"value": "col-lg-1", "label": "1/12"},
          {"value": "col-lg-2", "label": "2/12"},
          {"value": "col-lg-3", "label": "3/12"},
          {"value": "col-lg-4", "label": "4/12"},
          {"value": "col-lg-5", "label": "5/12"},
          {"value": "col-lg-6", "label": "6/12"},
          {"value": "col-lg-7", "label": "7/12"},
          {"value": "col-lg-8", "label": "8/12"},
          {"value": "col-lg-9", "label": "9/12"},
          {"value": "col-lg-10", "label": "10/12"},
          {"value": "col-lg-11", "label": "11/12"},
          {"value": "col-lg-12", "label": "12/12"},
          {"value": "col-lg-cus-5", "label": "1/5"}
        ]
      },
      {
        "type": "select",
        "id": "position_lg",
        "label": "Image position laptop",
        "default": "left",
        "info": "Breakpoint 992px ↑",
        "options": [
