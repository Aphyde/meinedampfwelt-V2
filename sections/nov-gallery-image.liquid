{%- liquid
  assign sst = section.settings
  assign bg_image = sst.background_image
-%}
{% style %}
  #shopify-section-{{ section.id }} .distance {
    padding-top: {{ sst.distance_top_xs }}px;
    padding-bottom: {{ sst.distance_bottom_xs }}px;
    padding-left: clamp(0px, {{ sst.px}}px, 15px);
    padding-right: clamp(0px, {{ sst.px}}px, 15px);
    {% if sst.background_color != blank or bg_image.src != blank %}
      background: {{ sst.background_color }}{% if bg_image.src != blank %} url("{{ bg_image | image_url: width: 1920 }}"){% endif %};
      background-size: cover;
    {% endif %}
  }
  @media(min-width: 768px) {
    #shopify-section-{{ section.id }} .distance {
      padding-top: {{ sst.distance_top_md }}px;
      padding-bottom: {{ sst.distance_bottom_md }}px;
      padding-left: {{ sst.px}}px;
      padding-right: {{ sst.px}}px;
    }
  }
  @media(min-width: 1200px) {
    #shopify-section-{{ section.id }} .distance {
      padding-top: {{ sst.distance_top_xl }}px;
      padding-bottom: {{ sst.distance_bottom_xl }}px;
    }
  }
  #shopify-section-{{ section.id }} .gallery-image__item {
    border-radius: {{ sst.bd_radius}}px;
  }
  {% for block in section.blocks %}
    {%- assign bst = block.settings -%}
    {% if sst.btn_style == 'btn' %}
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .btn {
        --color-button-text: {{ bst.btn_label_c }};
        --color-button-border: {{ bst.btn_label_br }};
        --color-button: {{ bst.btn_label_b }};
        --color-button-text-hover: {{ bst.btn_label_ch }};
        --color-button-border-hover: {{ bst.btn_label_bh }};
        --color-button-hover: {{ bst.btn_label_bh }};
      }
    {% else %}
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .link {
        --color-link: {{ bst.btn_label_c }};
        --color-hover: {{ bst.btn_label_ch }};
      }
    {% endif %}
    #shopify-section-{{ section.id }} .cap-{{ forloop.index }} {
      --vertical: {{ bst.position_vertical_xs }}%;
      --horizotal: {{ bst.position_horizotal_xs }}%;
      --width: {{ bst.w_xs }}%;
    }
    #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text1 {
      font-size: {{ bst.cta_label_n_xs }}px;
      line-height: {{ bst.cta_label_l }};
      letter-spacing: {{ bst.cta_label_letter }}px;
      margin-bottom: clamp(5px, {{ bst.cta_label_mb | times: 0.6 }}px, 20px);
    }
    #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text1 span {
      color: {{ bst.cta_label_span_c }}
    }
    #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 {
      font-size: {{ bst.cta_label2_n_xs }}px;
      line-height: {{ bst.cta_label2_l }};
      max-width: {{ bst.mw_2 }}px;
      margin-bottom: clamp(5px, {{ bst.cta_label2_mb | times: 0.6 }}px, 20px);
    }
    #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 a {
      color: {{ bst.cta_label2_c }};
    }
    #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 a:hover {
      color: var(--color-hover);
    }
    #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text3 {
      font-size: {{ bst.cta_label3_n_xs }}px;
      line-height: {{ bst.cta_label3_l }};
      max-width: {{ bst.mw_3 }}px;
      margin-bottom: clamp(5px, {{ bst.cta_label3_mb | times: 0.6 }}px, 20px);
    }
    @media(min-width: 576px) {
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} {
        --vertical: {{ bst.position_vertical_sm }}%;
        --horizotal: {{ bst.position_horizotal_sm }}%;
        --width: {{ bst.w_sm }}%;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text1 {
        font-size: {{ bst.cta_label_n_sm}}px;
        margin-bottom: clamp(5px, {{ bst.cta_label_mb | times: 0.7 }}px, 20px);
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 {
        font-size: {{ bst.cta_label2_n_sm}}px;
        margin-bottom: clamp(5px, {{ bst.cta_label2_mb | times: 0.7 }}px, 20px);
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text3 {
        font-size: {{ bst.cta_label3_n_sm}}px;
        margin-bottom: clamp(5px, {{ bst.cta_label3_mb | times: 0.7 }}px, 20px);
      }
    }
    @media(min-width: 768px) {
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} {
        --vertical: {{ bst.position_vertical_md }}%;
        --horizotal: {{ bst.position_horizotal_md }}%;
        --width: {{ bst.w_md }}%;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text1 {
        font-size: {{ bst.cta_label_n_md}}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 {
        font-size: {{ bst.cta_label2_n_md}}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text3 {
        font-size: {{ bst.cta_label3_n_md}}px;
      }
    }
    @media(min-width: 992px) {
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} {
        --vertical: {{ bst.position_vertical_lg }}%;
        --horizotal: {{ bst.position_horizotal_lg }}%;
        --width: {{ bst.w_lg }}%;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text1 {
        font-size: {{ bst.cta_label_n_lg }}px;
        margin-bottom: {{ bst.cta_label_mb | times: 0.8 }}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 {
        font-size: {{ bst.cta_label2_n_lg }}px;
        margin-bottom: {{ bst.cta_label2_mb | times: 0.8 }}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text3 {
        font-size: {{ bst.cta_label3_n_lg }}px;
        margin-bottom: {{ bst.cta_label3_mb | times: 0.8 }}px;
      }
    }
    @media(min-width: 1200px) {
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} {
        --vertical: {{ bst.position_vertical_xl }}%;
        --horizotal: {{ bst.position_horizotal_xl }}%;
        --width: {{ bst.w_xl }}%;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text1 {
        font-size: {{ bst.cta_label_n_xl}}px;
        margin-bottom: {{ bst.cta_label_mb }}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 {
        font-size: {{ bst.cta_label2_n_xl}}px;
        margin-bottom: {{ bst.cta_label2_mb }}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text3 {
        font-size: {{ bst.cta_label3_n_xl}}px;
        margin-bottom: {{ bst.cta_label3_mb }}px;
      }
    }
    @media(min-width: 1441px) {
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} {
        --vertical: {{ bst.position_vertical_xxl }}%;
        --horizotal: {{ bst.position_horizotal_xxl }}%;
        --width: {{ bst.w_xxl }}%;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text1 {
        font-size: {{ bst.cta_label_n_xxl}}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text2 {
        font-size: {{ bst.cta_label2_n_xxl}}px;
      }
      #shopify-section-{{ section.id }} .cap-{{ forloop.index }} .gallery-image__text3 {
        font-size: {{ bst.cta_label3_n_xxl}}px;
      }
    }
  {% endfor %}
{% endstyle %}
<div class="{% if sst.bg_size == 'full' %}distance {% endif %}{% if sst.class_section != blank %} {{ sst.class_section }}{% endif %}{% if sst.animate %} wow fadeInUp{% endif %}">
  <div class="{{ sst.container }}">
    <div class="{% if sst.bg_size == 'container' %}distance{% endif %}">
      {% render 'heading-section', sub_position: sst.subheading_position %}
      <div class="row {% if sst.spacing_1 %}spacing-1{% else %}spacing-{{ sst.spacing }} spacing-md-{{ sst.spacing_md }}{% endif %}">
        {% for block in section.blocks %}
          {% assign bst = block.settings %}
          {% if block.type == 'image' %}
            <div class="col-md gallery-image_column{% if sst.spacing_1 == false %} mb-{{ sst.spacing }}{% endif %}{% if bst.class != blank %} {{ bst.class }}{% endif %}">
              <div class="gallery-image__item overflow_hidden position-relative{% if sst.show_boxshadow %} box-shadow{% endif %}{% if bst.cta_label != blank or bst.btn_label != blank %} hs-cap{% endif %}{% if sst.style_img_1 %} nov-sh-image-1{% endif %}{% if sst.style_img_2 %} nov-sh-image-2{% endif %}">
                {% if bst.link != blank %}
                  <a class="w-100 gallery-image__link" href="{{ bst.link }}">
                {% endif %}
                {% if bst.image != blank %}
                  {% if bst.img_xs != blank %}
                    <div class="d-none d-sm-block">
                  {% endif %}
                  {% render 'nov-aspect-image', image: bst.image %}
                  {% if bst.img_xs != blank %}
                    </div>
                  {% endif %}
                {% else %}
                  {%- capture current -%}{% cycle 1, 2, 3 %}{%- endcapture -%}
                  {{ 'hero-apparel-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
                {% endif %}
                {% if bst.img_xs != blank %}
                  {% assign image_url = bst.img_xs %}
                  <div class="respone_image d-sm-none" style="padding-top:{{ 1 | divided_by: image_url.aspect_ratio | times: 100}}%;">
                    <img class="lazyload"
                      src="{{ image_url | image_url: width: 1 }}"
                      data-src="{{ image_url | image_url: width: 180 }}"
                      data-srcset="{{ image_url | image_url: width: 380 }} 380w,{{ image_url | image_url: width: 580 }} 580w" data-sizes="auto" alt="{{ image_url.alt }}" width="{{ image_url.width }}" height="{{ image_url.height }}" loading="lazy">
                  </div>
                {% endif %}
                {% if bst.link != blank %}
                  </a>
                {% endif %}
                {% if bst.pattern != blank %}
                  <div class="position-absolute pattern {{ bst.pattern_position }}" style="--vertical: {{ bst.pattern_position_vertical }}%;--horizotal: {{ bst.pattern_position_horizotal }}%; --width: {{ bst.width }}%">
                    <div class="img_animate">
                      <img data-src="{{ bst.pattern | image_url: width: 1024 }}" class="lazyload layer" alt="{{ bst.pattern.alt | default: 'image' }}" data-depth="0.4" width="1024" height="{{ 1024 | divided_by: bst.pattern.aspect_ratio }}" style="height: auto;" loading="lazy">
                    </div>
                  </div>
                {% endif %}
                {% if bst.cta_label != blank or bst.cta_label2 != blank or bst.btn_label != blank %}
                  <div class="gallery-image__caption position-absolute text-{{ bst.text_align }} cap-{{ forloop.index }} {{ bst.position }}">
                    <div class="image-caption__content">
                      {% if bst.cta_label != blank %}
                      <div class="gallery-image__text1 {{ bst.ff_1 }} {{ bst.cta_label_fw }}" style="color: {{ bst.cta_label_c }}">{{ bst.cta_label }}</div>
                      {% endif %}
                      {% if bst.cta_label2 != blank %}
                        <div class="gallery-image__text2 {{ bst.ff_2 }}" style="color: {{ bst.cta_label2_c }}">{{ bst.cta_label2 }}</div>
                      {% endif %}
                      {% if bst.cta_label3 != blank %}
                        <div class="gallery-image__text3 {{ bst.ff_3 }}" style="color: {{ bst.cta_label3_c }}">{{ bst.cta_label3 }}</div>
                      {% endif %}
                      {% if bst.btn_label != blank %}
                        {% if sst.btn_style == 'btn' %}
                          <a href="{{ bst.link }}" class="btn d-inline-flex box-shadow">
                            <span>{{ bst.btn_label }}</span>
                          </a>
                        {% else %}
                          <a href="{{ bst.link }}" class="link">
                            <span class="font-500">{{ bst.btn_label }}</span>
                            <i class="zmdi zmdi-long-arrow-right position-relative"></i>
                          </a>
                        {% endif %}
                      {% endif %}
                      {% if bst.show_newsletter %}
                        {% assign id = 'newsletter' | append: section.id | append: block.id %}
                        {% form 'customer', id: id %}
                          {{ form.errors | default_errors }}
                          {% if form.posted_successfully? %}
                          <p class="form--success text-center">{{ 'general.newsletter_form.confirmation' | t }}</p>
                          {% else %}
                          <div class="input-group">
                            <input type="hidden" name="contact[tags]" value="newsletter">
                            <input type="email"
                            name="contact[email]"
                            class="form-control input"
                            value="{% if customer %}{{ customer.email }}{% endif %}"
                            placeholder="{{ 'general.newsletter_form.email_newsletter' | t }}"
                            required
                            >
                            <button type="submit" class="newsletter__submit btn" name="commit" title="{{ 'general.newsletter_form.submit' | t }}">
                              <i class="zmdi zmdi-email"></i>
                            </button>
                          </div>
                          {% endif %}
                        {% endform %}
                      {% endif %}
                    </div>
                  </div>
                {% endif %}
              </div>
            </div>
          {% endif %}
          {% if block.type == '@app' %}
            <div class="col-md">
              {% render block %}
            </div>
          {% endif %}
        {% endfor %}
      </div><!-- End row -->
    </div>
  </div><!-- End container -->
</div>
{% if section.blocks.size == 0 %}
  {% render 'no-blocks' %}
{% endif %}
{% schema %}
  {
    "name": "Vinova Gallery Image",
    "class": "section-gallery-image",
    "max_blocks": 6,
    "settings": [
      {
        "type": "richtext",
        "id": "heading",
        "label": "Heading",
        "default": "<p>Vinova Gallery Image</p>"
      },
      {
        "type": "color",
        "id": "heading_color",
        "label": "Heading color",
        "default": "#222222"
      },
      {
        "type": "richtext",
        "id": "subheading",
        "label": "Sub heading",
        "default": "<p>Subtitle from happy customers</p>"
      },
      {
        "type": "color",
        "id": "subheading_color",
        "label": "Sub heading color",
        "default": "#666666"
      },
      {
        "type": "richtext",
        "id": "descheading",
        "label": "Description",
        "default": "<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>"
      },
      {
        "type": "color",
        "id": "descheading_color",
        "label": "Description color",
        "default": "#666666"
      },
      {
        "type": "select",
        "id": "subheading_position",
        "label": "Sub heading position",
        "default": "bottom",
        "options": [
          {
            "value": "top",
            "label": "Top"
          },
          {
            "value": "bottom",
            "label": "Bottom"
          }
        ]
      },
      {
        "type": "select",
        "id": "heading_alignment",
        "label": "Heading alignment",
        "default": "text-left",
        "options": [
          {
            "value": "text-left",
            "label": "Left"
          },
          {
            "value": "text-center",
            "label": "Center"
          },
          {
            "value": "text-right",
            "label": "Right"
          }
        ]
      },
      {
        "type": "header",
        "content": "Item Image Style"
      },
      {
        "type": "range",
        "id": "bd_radius",
        "min": 0,
        "max": 100,
        "step": 1,
        "unit": "px",
        "label": "Border radius image",
        "default": 0
      },
      {
        "type": "range",
        "id": "spacing",
        "min": 0,
        "max": 60,
        "step": 5,
        "label": "Spacing items",
        "default": 20
      },
      {
        "type": "range",
        "id": "spacing_md",
        "min": 0,
        "max": 40,
        "step": 5,
        "label": "Spacing items tablet",
        "default": 20,
        "info": "Breakpoint 991px ↓"
      },
      {
        "type": "checkbox",
        "id": "spacing_1",
        "label": "Spacing 1px"
      },
      {
        "type": "checkbox",
        "id": "show_boxshadow",
        "label": "Show box shadow"
      },
      {
        "type": "header",
        "content": "Style Hover Image"
      },
      {
        "type": "checkbox",
        "id": "style_img_1",
        "label": "Style hover image 1"
      },
      {
        "type": "checkbox",
        "id": "style_img_2",
        "label": "Style hover image 2",
        "default": true
      },
      {
        "type": "header",
        "content": "Button Style"
      },
      {
        "type": "select",
        "id": "btn_style",
        "label": "Button style",
        "default": "btn",
        "options": [
          { "value": "btn", "label": "Button" },
          { "value": "link", "label": "Link" }
        ]
      },
      {
        "type": "header",
        "content": "Section Style"
      },
      {
        "type": "checkbox",
        "id": "animate",
        "label": "Show animated",
        "default":   true
      },
      {
        "type": "select",
        "id": "container",
        "label": "Container",
        "default": "container",
        "options": [
          { "value": "container", "label": "Container" },
          { "value": "container-fluid", "label": "Container fluid" },
          { "value": "container-full", "label": "Container full width" }
        ]
      },
      {
        "type": "color",
        "id": "background_color",
        "label": "Background colors"
      },
      {
        "type": "image_picker",
        "id": "background_image",
        "label": "Background image"
      },
      {
        "type": "select",
        "id": "bg_size",
        "label": "Background size",
        "default": "full",
        "options": [
          { "value": "full", "label": "Full" },
          { "value": "container", "label": "Container"}
        ]
      },
      {
        "type": "text",
        "id": "class_section",
        "label": "Class"
      },
      {
        "type": "number",
        "id": "px",
        "label": "Horizontally spacing section (px)",
        "default": 0
      },
      {
        "type": "header",
        "content": "Spacing Top (px)"
      },
      {
        "type": "number",
        "id": "distance_top_xl",
        "label": "Large laptop",
        "default": 30,
        "info": "Breakpoint 1200px"
      },
      {
        "type": "number",
        "id": "distance_top_md",
        "label": "Tablet",
        "default": 30,
        "info": "Breakpoint 768px"
      },
      {
        "type": "number",
        "id": "distance_top_xs",
        "label": "Mobile",
        "default": 30
      },
      {
        "type": "header",
        "content": "Spacing Bottom (px)"
      },
      {
        "type": "number",
        "id": "distance_bottom_xl",
        "label": "Large laptop",
        "default": 30,
        "info": "Breakpoint 1200px"
      },
      {
        "type": "number",
        "id": "distance_bottom_md",
        "label": "Tablet",
        "default": 30,
        "info": "Breakpoint 768px"
      },
      {
        "type": "number",
        "id": "distance_bottom_xs",
        "label": "Mobile",
        "default": 30
      }
    ],
    "blocks": [
      {
        "type": "image",
        "name": "Image",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          },
          {
            "type": "image_picker",
            "id": "img_xs",
            "label": "Image mobile 575px ↓"
          },
          {
            "type": "url",
            "id": "link",
            "label": "Link"
          },
          {
            "type": "header",
            "content": "Pattern"
          },
          {
            "type": "image_picker",
            "id": "pattern",
            "label": "Pattern"
          },
          {
            "type": "select",
            "id": "pattern_position",
            "label": "Pattern position",
            "default": "right-center",
            "options": [
              { "value": "left-top", "label": "Left top" },
              { "value": "left-center", "label": "Left center"},
              { "value": "left-bottom", "label": "Left bottom"},
              { "value": "right-top", "label": "Right top" },
              { "value": "right-center", "label": "Right center"},
              { "value": "right-bottom", "label": "Right bottom"},
              { "value": "top-center", "label": "Top center"},
              { "value": "center-center", "label": "Center"},
              { "value": "bottom-center", "label": "Bottom center"}
            ]
          },
          {
            "type": "range",
            "id": "pattern_position_horizotal",
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "Position horizotal",
            "default": 0,
            "unit": "%"
          },
          {
            "type": "range",
            "id": "pattern_position_vertical",
            "min": 0,
            "max": 100,
            "step": 1,
            "label": "Position vertical",
            "default": 0,
            "unit": "%"
          },
          {
            "type": "range",
            "id": "width",
            "min": 10,
            "max": 100,
            "step": 1,
            "label": "Width",
            "default": 20,
            "unit": "%"
          },
          {
            "type": "header",
            "content": "Caption 1"
          },
          {
            "type": "text",
            "id": "cta_label",
            "label": "Caption",
            "default": "Lorem ipsum"
          },
          {
            "type": "color",
            "id": "cta_label_c",
            "label": "Color",
            "default": "#222222"
          },
          {
            "type": "color",
            "id": "cta_label_span_c",
            "label": "Color span",
            "default": "#222222"
          },
          {
            "type": "paragraph",
            "content": "Use for case: <span>Lorem</span> ipsum"
          },
          {
            "type": "number",
            "id": "cta_label_n_xxl",
            "label": "Font size desktop",
            "default": 22,
            "info": "Breakpoint 1440px"
          },
          {
            "type": "number",
            "id": "cta_label_n_xl",
            "label": "Font size large laptop",
            "default": 22,
            "info": "Breakpoint 1200px"
          },
          {
            "type": "number",
            "id": "cta_label_n_lg",
            "label": "Font size laptop",
            "default": 22,
            "info": "Breakpoint 992px"
          },
          {
            "type": "number",
            "id": "cta_label_n_md",
            "label": "Font size tablet",
            "default": 22,
            "info": "Breakpoint 768px"
          },
