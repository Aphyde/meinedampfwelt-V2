        "default": "nov-sh-image-2",
        "options": [
          { "value": "none", "label": "None"},
          { "value": "nov-sh-image-1", "label": "Style 1"},
          { "value": "nov-sh-image-2", "label": "Style 2"},
          { "value": "nov-sh-image-3", "label": "Style 3"},
          { "value": "nov-sh-image-4", "label": "Style 4"}
        ]
      },
      {
        "type": "header",
        "content": "Section Style"
      },
      {
        "type": "checkbox",
        "id": "animate",
        "label": "Show animated",
        "default":   true
      },
      {
        "type": "select",
        "id": "container",
        "label": "Container",
        "default": "container",
        "options": [
          { "value": "container", "label": "Container" },
          { "value": "container-fluid", "label": "Container fluid" },
          { "value": "container-full", "label": "Container full width" }
        ]
      },
      {
        "type": "color",
        "id": "background_color",
        "label": "Background colors"
      },
      {
        "type": "image_picker",
        "id": "background_image",
        "label": "Background image"
      },
      {
        "type": "select",
        "id": "bg_size",
        "label": "Background size",
        "default": "full",
        "options": [
          { "value": "full", "label": "Full" },
          { "value": "container", "label": "Container"}
        ]
      },
      {
        "type": "range",
        "id": "corner_radius",
        "min": 0,
        "max": 50,
        "step": 1,
        "unit": "px",
        "label": "Corner radius",
        "default": 0
      },
      {
        "type": "header",
        "content": "Pattern"
      },
      {
        "type": "image_picker",
        "id": "pattern",
        "label": "Pattern"
      },
      {
        "type": "select",
        "id": "pattern_position",
        "label": "Patter position",
