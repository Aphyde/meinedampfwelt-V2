{% assign sst = section.settings %}
{% style %}
  {%- assign bg_image = sst.background_image -%}
  #shopify-section-{{ section.id }} .distance {
    padding-top: {{ sst.distance_top_xs }}px;
    padding-bottom: {{ sst.distance_bottom_xs }}px;
    padding-left: clamp(0px, {{ sst.spacing_horizontal}}px, 15px);
    padding-right: clamp(0px, {{ sst.spacing_horizontal}}px, 15px);
    margin-top: {{ sst.mt_xs }}px;
  }
  {% if sst.background_color != blank or bg_image != blank %}
    #shopify-section-{{ section.id }} .distance:before {
      content: "";
      position: absolute;
      {% if sst.bg_position == 'top' %}
        top: 0;
      {% elsif sst.bg_position == 'center' %}
        top: 50%;
        transform: translateY(-50%);
      {% else %}
        bottom: 0;
      {% endif %}
      left: 0;
      width: 100%;
      height: {{ sst.height_background }}%;
      background: {{ sst.background_color }}{% if bg_image != blank %} url("{{ bg_image | image_url: width: 1920 }}"){% endif %} {{ sst.bg_img_position }} center;
      background-size: {{ sst.bg_img_size }};
    }
  {% endif %}
  #shopify-section-{{ section.id }} .collec-img__item {
    border-radius: {% if sst.border_radius_round %}9999999px{% else %}{{ sst.border_radius }}px{% endif %};
    background-color: {{ sst.bg_img }};
    padding: {{ sst.item_py }}px {{ sst.item_px }}px;
    {% if sst.show_border -%}
      border: 1px {{ sst.style_border }} {{ sst.c_border }};
    {%- endif %}
    {% if sst.item_max_width %}
      max-width: {{ sst.item_width }}px;
      margin: auto;
    {% endif %}
  }
  {% if sst.enable_img_width %}
    #shopify-section-{{ section.id }} .block-image {
      max-width: {{ sst.img_width }}px !important;
      margin: 0 auto;
    }
  {% endif %}
  {% if sst.enable_img_height %}
    #shopify-section-{{ section.id }} .respone_image {
      padding-top: {{ sst.img_height }}px !important;
    }
  {% endif %}
  {% if sst.enable_img_width or sst.enable_img_height %}
    #shopify-section-{{ section.id }} .respone_image img {
      object-fit: contain;
    }
  {% endif %}
  #shopify-section-{{ section.id }} .block-image {
    border-radius: {% if sst.border_radius_round %}9999999px{% else %}{{ sst.border_radius }}px {{ sst.border_radius }}px 0 0{% endif %};
    transform: translateZ(0);
  }
  #shopify-section-{{ section.id }} .respone_image img {
    border-radius: {% if sst.border_radius_round %}9999999px{% else %}{{ sst.border_radius }}px {{ sst.border_radius }}px 0 0{% endif %};
  }
  {% if sst.icon_hover != blank %}
    #shopify-section-{{ section.id }} .block-image:after {
      border-radius: {% if sst.border_radius_round %}9999999px{% else %}{{ sst.border_radius }}px{% endif %};
    }
  {% endif %}
  #shopify-section-{{ section.id }} .collec-img__text,
  #shopify-section-{{ section.id }} .collec-img__text a {
    color: {{ sst.h_c }};
  }
  #shopify-section-{{ section.id }} .collec-img__desc,
  #shopify-section-{{ section.id }} .collec-img__desc a {
    color: {{ sst.desc_c }};
  }
  @media(min-width: 768px) {
    #shopify-section-{{ section.id }} .distance {
      padding-top: {{ sst.distance_top_md }}px;
      padding-bottom: {{ sst.distance_bottom_md }}px;
      padding-left: clamp(0px, {{ sst.spacing_horizontal}}px, 30px);
      padding-right: clamp(0px, {{ sst.spacing_horizontal}}px, 30px);
      margin-top: {{ sst.mt_md }}px;
    }
  }
  @media(min-width: 1200px) {
    #shopify-section-{{ section.id }} .distance {
      padding-top: {{ sst.distance_top_xl }}px;
      padding-bottom: {{ sst.distance_bottom_xl }}px;
      padding-left: {{ sst.spacing_horizontal }}px;
      padding-right: {{ sst.spacing_horizontal }}px;
      margin-top: {{ sst.mt_xl }}px;
    }
  }
  {% if sst.show_pagination_progress %}
    #shopify-section-{{ section.id }} .swiper-scrollbar {
      background: {{ sst.pagination_progress_bg }} !important;
    }
    #shopify-section-{{ section.id }} .swiper-scrollbar-drag {
      background: {{ sst.progress_bg }} !important;
    }
  {% endif %}
{% endstyle %}
<div class="{% if sst.bg_size == 'full' %}distance{% endif %} color-{{ sst.color_section }}{% if sst.class_section != blank %} {{ sst.class_section }}{% endif %}{% if sst.animate %} wow fadeInUp{% endif %} position-relative {{ sst.type }}" data-section-id="{{ section.id }}" data-section-type="nov-swiper">
  <div class="{{ sst.container }}">
    {% if sst.pattern1 != blank and sst.container == 'container' %}
      <div class="img-pattern-side pattern1 position-absolute">
        <img data-src="{{ sst.pattern1 | image_url: width: 1920 }}" class="lazyload img-fluid" width="{{ sst.pattern1.width }}" height="{{ sst.pattern1.height }}">
      </div>
    {% endif %}
    {% if sst.pattern2 != blank and sst.container == 'container' %}
      <div class="img-pattern-side pattern2 position-absolute">
        <img data-src="{{ sst.pattern2 | image_url: width: 1920 }}" class="lazyload img-fluid" width="{{ sst.pattern2.width }}" height="{{ sst.pattern2.height }}">
      </div>
    {% endif %}
    <div class="{% if sst.bg_size == 'container' %}distance {% endif %}position-relative">
      {% render 'heading-section', sub_position: sst.subheading_position %}

      <div class="nov-swiper-carousel swiper{% if sst.show_dot %} pb-40{% endif %}{% if sst.show_border %} show-border{% endif %}{% if sst.show_box_shadow %} show-box-shadow{% endif %}{% if sst.icon_hover != blank %} has-icon-hover{% endif %} animate-{{ sst.animate_hover }}"
        data-autoplay="false" 
        data-autoplayTimeout="6000" 
        data-loop="false"
        data-dots="{{ sst.show_dots }}"
        data-nav="{{ sst.show_nav }}"
        data-row="1"
        data-items_xxl="{{ sst.xxl }}"
        data-items_xl="{{ sst.xl }}"
        data-items_lg="{{ sst.lg }}"
        data-items_md="{{ sst.md }}"
        data-items_sm="{{ sst.sm }}"
        data-items_xs="{{ sst.xs }}"
        data-spacing="{{ sst.spacing }}">
        <div class="swiper-wrapper">
          {% for block in section.blocks %}
            {% assign bst = block.settings %}
            <div class="swiper-slide sp-item">
              {% if block.type == 'image' %}
                <div class="collec-img__item">
                  <div class="block-image{% if bst.bg_img != blank %} bg-img position-relative {{ bst.bg_img_position }}{% endif %}"{% if bst.bg_img != blank %} style="--bg: {{ bst.bg_img }}; --height-bg:{{ bst.bg_img_height }}%; --corner: {% if sst.border_radius_round %}9999999px{% else %}{{ sst.border_radius }}px{% endif %}"{% endif %}>
                    {% if sst.icon_hover != blank %}
                      <img data-src="{{ sst.icon_hover | image_url: width: 50 }}" class="icon-hover img-fluid lazyload" alt="{{ sst.icon_hover.alt }}" width="50" height="50">
                    {% endif %}
                    <a href="{{ bst.link }}" class="d-block{% if sst.enable_img_height %} h-100{% endif %}">
                      {% if bst.image != blank %}
                        {% render 'nov-aspect-image', image: bst.image %}
                      {% else %}
                        {%- capture current -%}{% cycle 1, 2, 3, 4 %}{%- endcapture -%}
                        {{ 'collection-apparel-' | append: current | placeholder_svg_tag: 'placeholder-svg' }}
                      {% endif %}
                    </a>
                    {% if sst.pattern_img %}
                      <div class="pattern-img position-absolute bottom-center">
                        <svg width="234" height="35" viewBox="0 0 234 35" fill="none" xmlns="http://www.w3.org/2000/svg">
                        <path d="M51 15C41 19.4 28 26.5 0 35H234C214 29.5 195.399 20.2356 183.5 15C171 9.5 150 0 117 0C84 0 63.5 9.5 51 15Z" fill="{{ sst.pattern_bg }}"/>
                        </svg>
                        {% if sst.show_icon_pattern %}
                          <a href="{{ bst.link }}" class="icon-pattern d-flex align-items-center justify-content-center">
                            <span class="d-flex align-items-center justify-content-center">
                              <i class="rbb-icon-store-16"></i>
                            </span>
                          </a>
                        {% endif %}
                      </div>
                    {% endif %}
                  </div>
                  <div class="collec-img__content {{ sst.block_alignment }}">
                    {% if bst.cta_label != blank %}
                      <div class="collec-img__text {{ sst.fz }} {{ sst.fw }} {{ sst.ff }} mt-25">
                        {% if sst.cta_label_type == 'link' %}
                          <a href="{{ bst.link }}">
                        {% endif %}
                          {{ bst.cta_label }}
                        {% if sst.cta_label_type == 'link' %}
                          </a>
                        {% endif %}
                      </div>
                    {% endif %}
                    {% if bst.desc != blank %}
                      <div class="collec-img__desc {{ sst.fz_desc }} {{ sst.fw_desc }} mt-10">{{ bst.desc }}</div>
                    {% endif %}
                    {% if bst.btn != blank %}
                      <a href="{{ bst.link }}" class="btn {{ sst.btn_type }} {{ sst.btn_size }} box-shadow mt-10">
                        <span>{{ bst.btn }}</span>
                      </a>
                    {% endif %}
                  </div>
                </div>
              {% endif %}
              {% if block.type == '@app' %}
                {% render block %}
              {% endif %}
            </div>
          {% endfor %}
        </div>
        {% if sst.show_pagination_progress %}
          <div class="swiper-scrollbar mt-80 mt-md-60 position-static"></div>
        {% endif %}
      </div>
    </div>
  </div><!-- End container -->
</div>
{% if section.blocks.size == 0 %}
  {% render 'no-blocks' %}
{% endif %}

{% schema %}
  {
    "name": "Vinova Image Link",
    "class": "section-image-link",
    "settings": [
      {
        "type": "richtext",
        "id": "heading",
        "label": "Heading",
        "default": "<p>Vinova Image Link</p>"
      },
      {
        "type": "color",
        "id": "heading_color",
        "label": "Heading color",
        "default": "#222222"
      },
      {
        "type": "richtext",
        "id": "subheading",
        "label": "Sub heading",
        "default": "<p>Subtitle from happy customers</p>"
      },
      {
        "type": "color",
        "id": "subheading_color",
        "label": "Sub heading color",
        "default": "#666666"
      },
      {
        "type": "richtext",
        "id": "descheading",
        "label": "Description",
        "default": "<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>"
      },
      {
        "type": "color",
        "id": "descheading_color",
        "label": "Description color",
        "default": "#666666"
      },
      {
        "type": "select",
        "id": "subheading_position",
        "label": "Sub heading position",
        "default": "bottom",
        "options": [
          {
            "value": "top",
            "label": "Top"
          },
          {
            "value": "bottom",
            "label": "Bottom"
          }
        ]
      },
      {
        "type": "select",
        "id": "heading_alignment",
        "label": "Heading alignment",
        "default": "text-left",
        "options": [
          {
            "value": "text-left",
            "label": "Left"
          },
          {
            "value": "text-center",
            "label": "Center"
          },
          {
            "value": "text-right",
            "label": "Right"
          }
        ]
      },
      {
        "type":  "header",
        "content": "Item Per Row"
      },
      {
        "type": "range",
        "id": "xxl",
        "min": 1,
        "max": 10,
        "step": 1,
        "label": "Desktop",
        "default": 3,
        "info": "Breakpoint 1440px"
      },
      {
        "type": "range",
        "id": "xl",
        "min": 1,
        "max": 10,
        "step": 1,
        "label": "Large laptop",
        "default": 3,
        "info": "Breakpoint 1200px"
      },
      {
        "type": "range",
        "id": "lg",
        "min": 1,
        "max": 8,
        "step": 1,
        "label": "Laptop",
        "default": 3,
        "info": "Breakpoint 992px"
      },
      {
        "type": "range",
        "id": "md",
        "min": 1,
        "max": 7,
        "step": 1,
        "label": "Tablet",
        "default": 3,
        "info": "Breakpoint 768px"
      },
      {
        "type": "range",
        "id": "sm",
        "min": 1,
        "max": 6,
        "step": 1,
        "label": "Large mobile",
        "default": 2,
        "info": "Breakpoint 576px"
      },
      {
        "type": "range",
        "id": "xs",
        "min": 1,
        "max": 6,
        "step": 1,
        "label": "Mobile",
        "default": 2
      },
      {
        "type": "checkbox",
        "id": "show_dots",
        "label": "Show dots slider",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "show_nav",
        "label": "Show nav",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "show_pagination_progress",
        "label": "Show pagination progress",
        "default": false
      },
      {
        "type": "color",
        "id": "pagination_progress_bg",
        "label": "Background pagination progress",
        "default": "#eaeaee"
      },
      {
        "type": "color",
        "id": "progress_bg",
        "label": "Background progress",
        "default": "#000"
      },
      {
        "type": "range",
        "id": "spacing",
        "min": 0,
        "max": 100,
        "step": 5,
        "unit": "px",
        "label": "Spacing",
        "default": 30,
        "info": "Spacing item"
      },
      {
        "type": "header",
        "content": "Style Item"
      },
      {
        "type": "checkbox",
        "id": "item_max_width",
        "label": "Item max width",
        "default": false
      },
      {
        "type": "number",
        "id": "item_width",
        "label": "Item width (px)",
        "default": 250
      },
      {
        "type": "range",
        "id": "item_px",
        "min": 0,
        "max": 80,
        "step": 1,
        "label": "Padding X",
        "default": 0,
        "unit": "px"
      },
      {
        "type": "range",
        "id": "item_py",
        "min": 0,
        "max": 80,
        "step": 1,
        "label": "Padding Y",
        "default": 0,
        "unit": "px"
      },
      {
        "type": "color",
        "id": "bg_img",
        "label": "Background color",
        "default": "#f2f2f2"
      },
      {
        "type": "range",
        "id": "border_radius",
        "min": 0,
        "max": 100,
        "step": 1,
        "unit": "px",
        "label": "Border radius",
        "default": 0
      },
      {
        "type": "checkbox",
        "id": "border_radius_round",
        "label": "Round item"
      },
      {
        "type": "select",
        "id": "animate_hover",
        "label": "Item animate when hover",
        "default": "zoom_img",
        "options": [
          { "value": "zoom_img", "label": "Zoom image"},
          { "value": "slide_up", "label": "Slider up"},
          { "value": "none", "label": "None"}
        ]
      },
      {
        "type": "header",
        "content": "Icon When Hover"
      },
      {
        "type": "image_picker",
        "id": "icon_hover",
        "label": "Icon"
      },
      {
        "type": "header",
        "content": "Image"
      },
      {
        "type": "checkbox",
        "id": "enable_img_width",
        "label": "Enable width custom"
      },
      {
        "type": "number",
        "id": "img_width",
        "label": "Image width (px)"
      },
      {
        "type": "checkbox",
        "id": "enable_img_height",
        "label": "Enable height custom"
      },
      {
        "type": "number",
        "id": "img_height",
        "label": "Image height (px)"
      },
      {
        "type": "header",
        "content": "Border Item"
      },
      {
        "type": "checkbox",
        "id": "show_border",
        "label": "Show border"
      },
      {
        "type": "select",
        "id": "style_border",
        "label": "Border style",
        "options": [
          {
            "value": "solid",
            "label": "Solid"
          },
          {
            "value": "dashed",
            "label": "Dashed"
          },
          {
            "value": "dotted",
            "label": "Dotted"
          }
        ]
      },
      {
        "type": "color",
        "id": "c_border",
        "label": "Border color",
        "default": "#ededed"
      },
      {
        "type": "header",
        "content": "Box Shadow Item"
      },
      {
        "type": "checkbox",
        "id": "show_box_shadow",
        "label": "Show box shadow"
      },
      {
        "type": "header",
        "content": "Block Text"
      },
      {
        "type": "color",
        "id": "h_c",
        "label": "Heading color",
        "default": "#222"
      },
      {
        "type": "select",
        "id": "ff",
        "label": "Font family",
        "default": "f_df",
        "options": [
          {"value": "f_df","label": "Body font"},
          {"value": "f_pr","label": "Heading font"}
        ]
      },
      {
        "type": "select",
        "id": "fz",
        "label": "Heading font size",
        "default": "h4",
        "options": [
          { "value": "h1", "label": "H1" },
          { "value": "h2", "label": "H2" },
          { "value": "h3", "label": "H3" },
          { "value": "h4", "label": "H4" },
          { "value": "h5", "label": "H5" },
          { "value": "h6", "label": "H6" }
        ]
      },
      {
        "type": "select",
        "id": "fw",
        "label": "Heading font weight",
        "default": "font-800",
        "options": [
          {"value": "font-300","label": "300 - Light"},
          {"value": "font-400","label": "400 - Regular"},
          {"value": "font-500","label": "500 - Medium"},
          {"value": "font-600","label": "600 - SemiBold"},
          {"value": "font-700","label": "700 - Bold"},
          {"value": "font-800","label": "800 - ExtraBold"},
          {"value": "font-900","label": "900 - Black"}
        ]
      },
      {
        "type": "select",
        "id": "cta_label_type",
        "label": "Heading type",
        "default": "link",
        "options": [
          { "value": "link", "label": "Link" },
          { "value": "text", "label": "Text"}
        ]
      },
      {
        "type": "paragraph",
        "content": "==================="
      },
      {
        "type": "color",
        "id": "desc_c",
        "label": "Description color",
        "default": "#222"
      },
      {
        "type": "select",
        "id": "fz_desc",
        "label": "Description font size",
        "default": "h4",
        "options": [
          { "value": "h1", "label": "H1" },
          { "value": "h2", "label": "H2" },
          { "value": "h3", "label": "H3" },
          { "value": "h4", "label": "H4" },
          { "value": "h5", "label": "H5" },
          { "value": "h6", "label": "H6" }
        ]
      },
      {
        "type": "select",
        "id": "fw_desc",
        "label": "Description font weight",
        "default": "font-400",
        "options": [
          {"value": "font-300","label": "300 - Light"},
          {"value": "font-400","label": "400 - Regular"},
          {"value": "font-500","label": "500 - Medium"},
          {"value": "font-600","label": "600 - SemiBold"},
          {"value": "font-700","label": "700 - Bold"},
          {"value": "font-800","label": "800 - ExtraBold"},
          {"value": "font-900","label": "900 - Black"}
        ]
      },
      {
        "type": "paragraph",
        "content": "==================="
      },
      {
        "type": "select",
        "id": "btn_type",
        "label": "Button type",
        "default": "btn-primary",
        "options": [
          { "value": "btn-primary", "label": "Primary" },
          { "value": "btn-secondary", "label": "Secondary" }
        ]
      },
      {
        "type": "select",
        "id": "btn_size",
        "label": "Button size",
        "default": "btn-normal",
        "options": [
          { "value": "btn-normal", "label": "Normal" },
          { "value": "btn-medium", "label": "Medium" },
          { "value": "btn-large", "label": "Large" }
        ]
      },
      {
        "type": "paragraph",
        "content": "==================="
      },
      {
        "type": "checkbox",
        "id": "pattern_img",
        "label": "Show pattern"
      },
      {
        "type": "color",
        "id": "pattern_bg",
        "label": "Background pattern",
        "default": "#fff"
      },
      {
        "type": "checkbox",
        "id": "show_icon_pattern",
        "label": "Icon on pattern",
        "default": true
      },
      {
        "type": "paragraph",
        "content": "==================="
      },
      {
        "type": "select",
        "id": "block_alignment",
        "label": "Block text alignment",
        "default": "text-left",
        "options": [
          {
            "value": "text-left",
            "label": "Left"
          },
          {
            "value": "text-center",
            "label": "Center"
          },
          {
            "value": "text-right",
            "label": "Right"
          }
        ]
      },
      {
        "type": "header",
        "content": "Section Style"
      },
      {
        "type": "checkbox",
        "id": "animate",
        "label": "Show animated",
        "default":   true
      },
      {
        "type": "select",
        "id": "container",
        "label": "Container",
        "default": "container",
        "options": [
          { "value": "container", "label": "Container" },
          { "value": "container-fluid", "label": "Container fluid" },
          { "value": "container-full", "label": "Container full width" }
        ]
      },
      {
        "type": "color_scheme",
        "id": "color_section",
        "label": "Section color",
        "default": "background-1"
      },
      {
        "type": "color",
        "id": "background_color",
        "label": "Background colors"
      },
      {
        "type": "image_picker",
        "id": "background_image",
        "label": "Background image"
      },
      {
        "type": "select",
        "id": "bg_img_size",
        "label": "Background image size",
        "default": "cover",
        "options": [
          { "value": "contain", "label": "Contain" },
          { "value": "cover", "label": "Cover" },
          { "value": "auto", "label": "Auto" }
        ]
      },
      {
        "type": "select",
        "id": "bg_img_position",
        "label": "Background image position",
        "default": "center",
        "options": [
          { "value": "top", "label": "Top" },
          { "value": "center", "label": "Center" },
          { "value": "bottom", "label": "Bottom" }
        ]
      },
      {
        "type": "range",
        "id": "height_background",
        "min": 30,
        "max": 100,
